body {
margin-top: 0px;
background-color: #DCE8DD; 
background-repeat: repeat-y;
background-repeat: repeat-x;
color: black;
}
h1, h2, h3, th{
color: #B89610;
} 

#header {
background-color: green;
height: 100px;
text-align: center;
padding-top: 20px;
}
#header h1 {
margin-right: auto;
margin-left: auto;
text-align: center;
font-size: 2em;
color: #FFFFFF;
}

#leftWing {
width: 150px;
position: absolute;
left: 0px;
top: 28px;
margin-top: 135px;
border-top-width: 2px;
border-top-style: solid;
border-top-color: #0066CC;
margin-left: 15px;
border-bottom-width: 2px;
border-bottom-style: solid;
border-bottom-color: #0066CC;
}

#rightWing {
width: 150px;
position: absolute;
top: 28px;
right: 0px;
margin-top: 135px;
border-top-width: 2px;
border-top-style: solid;
border-top-color: #0066CC;
margin-right: 15px;
border-bottom-width: 2px;
border-bottom-style: solid;
border-bottom-color: #0066CC;
}

#content {
margin-left: 175px;
margin-right: 175px;
}

#content h5 {
font-family: Arial, Helvetica, sans-serif;
font-size: 15px;
text-align: center;
text-decoration: underline;
}

.navbar-inner {
background: #23470a; /* Old browsers */
background: -moz-linear-gradient(top, #23470a 59%, #517816 92%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(59%,#23470a), color-stop(92%,#517816)); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top, #23470a 59%,#517816 92%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top, #23470a 59%,#517816 92%); /* Opera 11.10+ */
background: -ms-linear-gradient(top, #23470a 59%,#517816 92%); /* IE10+ */
background: linear-gradient(to bottom, #23470a 59%,#517816 92%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#23470a', endColorstr='#517816',GradientType=0 ); /* IE6-9 */
}

.navbar .nav .active > a,
.navbar .nav .active > a:active {
  color: #ffffff;
  text-decoration: none;
  
}

.course-set{
	/*background-color: #ffffff;*/
	background-color: #F5F5F5;
	margin-bottom: 3px;
	padding: 2px;
	font-weight:normal;
}
.slo-result{
//	background-color: #F2FCF2;
	margin-bottom: 3px;
	padding: 2px;
	padding-left: 7px;
	font-size: 90%;
	font-weight: normal;
}	
.keyword{
/*	Removed options
	background-color: #FFF1C4;
	font-style: italic;
*/
	font-weight:bold;
	font-size: 120%;
}

.well {
  min-height: 20px;
  padding: 19px;
  margin-bottom: 20px;
  background-color: #f5f5f5;
  /* box background #f5f5f5 */
  border: 1px solid #ffffff;
  border: 1px solid rgba(0, 0, 0, 0.05);
  -webkit-border-radius: 4px;
     -moz-border-radius: 4px;
          border-radius: 4px;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.05);
     -moz-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.05);
          box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.05);
}

p
{
	color: black;
}

.sub{
	font-size: 80%;
}

#tag-cloud{
	padding: 5px;
	margin-bottom: 15px;
	line-height: 300%;
	
}

#tag-cloud a{
	color:#ffffff;
	text-decoration: none;
}
	
#tag-cloud a:hover{
	color: #FFFFFF;
}
.right-align{
	text-align: right;

}
.left-align{
	text-align: left;
}

.tag{
	padding-top: 2px;
	background-color: ##55891A;
	font-weight: normal;
}
.tag:hover{
	background-color: #EAA800;
}

.cloud-0{
	font-size: 100%;
}
.cloud-1{
	font-size: 125%;
}
.cloud-2{
	font-size: 150%;
}
.cloud-3{
	font-size: 175%;
}
.cloud-4{
	font-size: 200%;
}



