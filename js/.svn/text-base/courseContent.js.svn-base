
jQuery(document).ready(function() {


	//for course tabs
	$(function () {
			$("#courseTabs a:first").tab("show");
		})
		
	$("#courseTabs a").click(function (e) {
		e.preventDefault();
		$(this).tab("show");
	})
	
	
	//for SLO Listing tabs
	$(function () {
			$("#browseSLOTabs a:first").tab("show");
		})
		
	$("#browseSLOTabs a").click(function (e) {
		e.preventDefault();
		$(this).tab("show");
	})
	
	//for SLO tabs
	$(function () {
			$("#sloTabs a:first").tab("show");
		})
		
	$("#sloTabs a").click(function (e) {
		e.preventDefault();
		$(this).tab("show");
	})
	
});